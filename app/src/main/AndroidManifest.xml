<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tarefapmdm02">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Administrador">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".Compras_realizadas" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".cliente" />
        </activity>

        <activity android:name=".Pedidos_Tramite">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".cliente" />
        </activity>

        <activity android:name=".finalizarPedido" >
           <meta-data
              android:name="android.support.PARENT_ACTIVITY"
              android:value=".Facer_Pedido" />
        </activity>

        <activity android:name=".Facer_Pedido">
           <meta-data
              android:name="android.support.PARENT_ACTIVITY"
              android:value=".cliente" />
        </activity>

        <activity android:name=".cliente">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>